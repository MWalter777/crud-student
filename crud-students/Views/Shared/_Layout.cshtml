<!DOCTYPE html>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top nav-custom">
        <div class="container">
            <div class="navbar-header">
                @Html.ActionLink("Students", "/", "Students", new { area = "" }, new { @class = "navbar-brand" })
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Index", "/", "Home")</li>
                    <li>@Html.ActionLink("Students", "/", "Students")</li>
                    <li>@Html.ActionLink("Subjects", "/", "Subjects")</li>
                    <li>@Html.ActionLink("Teachers", "/", "Teachers")</li>
                    <li>@Html.ActionLink("Studentsubjects", "/", "Studentsubjects")</li>
                    <li>@Html.ActionLink("Subjectteachers", "/", "Subjectteachers")</li>
                </ul>
            </div>
        </div>
    </div>

    <div class="content-custom">
        <div class="container students-index" id="principal">
            @RenderBody()
        </div>
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
    <script>
        $(document).ready(function () {
            $('#myTable').DataTable();
        });
    </script>
</body>
</html>
